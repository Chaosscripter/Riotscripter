local Window = OrionLib:MakeWindow({
    Name = "Riotscripter | The Gyyaattt Simulator [" .. os.date("%Y") .. "]",
    HidePremium = false,
    SaveConfig = false,
    ConfigFolder = "Riotscripter",
    IntroEnabled = true,
    IntroText = "Welcome To Riotscripter's The Gyyaattt Simulator Gui.",
})

local AutoFarmTab = Window:MakeTab({
    Name = "Auto Farm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false,
})

AutoFarmTab:AddToggle({
    Name = "Auto Weights",
    Default = false,
    Save = true,
    Flag = "autoweights_tgs",
    Callback = function(Value)
        getgenv().AutoWeights = Value
        autoweights()
    end
})

