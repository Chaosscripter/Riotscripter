            end
          end
        end
      end
    end

    function autograbrings4()
      while getgenv().AutoGrabRings4 == true do
        task.wait(Cooldown)
        if game:GetService("Workspace"):FindFirstChild("Ring") or game:GetService("Workspace"):FindFirstChild("SpecialRing") then
          for i, v in ipairs(workspace:GetChildren()) do
            if (v.Name == "Ring" or v.Name == "SpecialRing") then
              firetouchinterest(v, Head, 0)
              task.wait()
              firetouchinterest(v, Head, 1)
            end
          end
        end
      end
    end

