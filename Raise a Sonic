        task.wait()
      end
    end

    function autofish()
      while getgenv().AutoFish == true do
        Player.Character:SetPrimaryPartCFrame(CFrame.new(Vector3.new(105.742981, -35.6520615, -15.4310207, 0, 0, 1, -1, 0, 0, 0, -1, 0)))
        task.wait(1)
        Player.Character:SetPrimaryPartCFrame(CFrame.new(Vector3.new(105.742981, -38.6520615, -15.4310207, 0, 0, 1, -1, 0, 0, 0, -1, 0)))
        task.wait()
      end
      Player.Character:SetPrimaryPartCFrame(originalCFrame)
    end

    function autograbrings()
      while getgenv().AutoGrabRings do
        task.wait(Cooldown)
        if game:GetService("Workspace"):FindFirstChild("Ring") then
          for i, v in next, Workspace:GetChildren() do
            if v.Name == "Ring" and v then
              firetouchinterest(LocalPlayer.Character.Head, v, 0)
              firetouchinterest(LocalPlayer.Character.Head, v, 1)
            end
          end
        end
      end
    end

    function autograbrings2()
